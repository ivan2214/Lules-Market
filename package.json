    "dev": "bun --bun next dev",
    "build": "bun --bun next build",
    "start": "bun --bun next start",
    "lint": "biome check",
    "format": "biome check --write",
    "check": "biome ci .",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:seed": "bun src/db/seed.ts",
    "db:studio": "drizzle-kit studio",
    "db:reset": "bun src/db/reset.ts && bun db:generate && bun db:migrate && bun db:push",
    "test:unit": "vitest",
    "test:e2e": "bunx playwright test",
    "test:e2e:ui": "bunx playwright test --ui",
    "test:e2e:chromium": "bunx playwright test --project=chromium",
    "test:e2e:debug": "bunx playwright test --debug",
    "test:e2e:file": "bunx playwright test",
    "test:e2e:codegen": "bunx playwright codegen"
  },
